		} else {
			n_out6p0.y = (1.0 - (1.0 - base) * (1.0 - 2.0 * (blend - 0.5)));
		}
	}
	{
		float base = n_out2p0.z;
		float blend = n_out8p0.z;
		if (base < 0.5) {
			n_out6p0.z = (base * (2.0 * blend));
		} else {
			n_out6p0.z = (1.0 - (1.0 - base) * (1.0 - 2.0 * (blend - 0.5)));
		}
	}

// Output:0
	COLOR.rgb = n_out6p0;

}

