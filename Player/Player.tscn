// ScalarUniform:5
	float n_out5p0 = blur_amount;

// Input:3

// Texture:2
	vec3 n_out2p0;
	float n_out2p1;
	{
		vec4 _tex_read = textureLod(SCREEN_TEXTURE, n_out4p0.xy, n_out5p0);
		n_out2p0 = _tex_read.rgb;
		n_out2p1 = _tex_read.a;
	}

// Color:8
	vec3 n_out8p0 = vec3(0.356863, 0.356863, 0.356863);
	float n_out8p1 = 1.000000;

// ColorOp:6
	vec3 n_out6p0;
	{
		float base = n_out2p0.x;
		float blend = n_out8p0.x;
		if (base < 0.5) {
			n_out6p0.x = (base * (2.0 * blend));
		} else {
			n_out6p0.x = (1.0 - (1.0 - base) * (1.0 - 2.0 * (blend - 0.5)));
		}
	}
	{
		float base = n_out2p0.y;
		float blend = n_out8p0.y;
		if (base < 0.5) {
